  width: 100%;
  height: 100%;
  /* min-height: 100%; */
  transform: translate(-50%, -50%);
  background-image: url('shousha/bg_lobby.jpg');
  font-family: 'fzbwks';
  text-shadow: none;
  
}
.qh-window {
  position: absolute;
  height:100%;
  width:100%;
  left: 50%;
  top: 50%;
  display: block;
  background-size: 100% 100%;
  background-repeat: no-repeat;
  transform: translate(-50%, -50%);
